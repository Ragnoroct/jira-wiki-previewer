{
    "name": "jira-wiki-previewer",
    "displayName": "jira-wiki-previewer",
    "description": "",
    "version": "0.0.1",
    "publisher": "ragnoroct",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showJiraWikiPreview"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showJiraWikiPreview",
                "title": "Show Jira Wiki Preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "lit-html": "^0.10.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "unfetch": "^3.0.0",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@types/request": "^2.47.1",
        "nonce": "^1.0.4",
        "request": "^2.87.0"
    }
}
